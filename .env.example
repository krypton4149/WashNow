# Copy to .env and fill in your values. Do not commit .env to git.
# API
API_URL=https://your-api-domain.com
API_BASE_URL=https://your-api-domain.com/api/v1
STORAGE_BASE_URL=https://your-api-domain.com/public/storage

# Stripe (use test keys for development)
STRIPE_KEY=pk_test_xxx
STRIPE_SECRET=sk_test_xxx
STRIPE_WEBHOOK_SECRET=whsec_xxx

# Apple Pay
MERCHANT_IDENTIFIER=org.reactjs.native.example.Cars

# PayPal
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_WEBHOOK_ID=your_webhook_id
